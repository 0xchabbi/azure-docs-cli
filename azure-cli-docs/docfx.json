{
  "build": {
    "content": [
      {
        "files": [
          "**/*.pycliyml",
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "cli/azure/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "breadcrumb.json"
        ],
        "exclude": [
          "**/obj/**",
          "cli/azure/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/cli/azure/breadcrumb.json",
      "brand": "azure"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "cli/azure"
  }
}
