{
  "build": {
    "content": [
      {
        "src": "docs-ref-autogen",
        "files": [
          "**/*.pycliyml",
          "**/*.md"
        ],
        "exclude": [
          "refTOC.md",
          "**/obj/**",
          "cli/**",
          "**/includes/**"
        ]
      },
      {
        "src": "docs-ref-conceptual",
        "files": [
          "**.md"
        ],
        "exclude":[
          "toc.md",
          "**/toc.md"
        ]
      },
      {
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "files": [
          "**/*.yml"
        ]
      }

    ],
    "resource": [
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/cli/azure/breadcrumb/toc.json",
      "keywords": "azure, cli, command line",
      "brand": "azure",
      "author": "clark-allen",
      "ms.author": "allclark",
      "manager": "douge",
      "ms.topic": "reference",
      "ms.devlang": "python",
      "ms.date": "12/06/2016"
    },
    "fileMetadata": {
       "ms.service":
        {
          "acr/**": "container-registry",
          "acs/**": "container-service",
          "ad/**": "active-directory",
          "appservice/**": "app-service",
          "container/**": "container-service",
          "group/**": "azure-resource-manager",
          "iot/**": "iot-hub",
          "keyvault/**":"key-vault",
          "network/**":"virtual-network",
          "policy/**": "",
          "redis/**":"redis-cache",
          "resource/**":"azure-resource-manager",
          "role/**": "",
          "sql/**": "sql-server",
          "storage/**": "storage",
          "vm/**": "virtual-machines",
          "vmss/**": "virtual-machine-scale-sets"    
         },
        "hideEdit":
        {
          "docs-ref-autogen/**": "true"
        }
    },
    "template": [],
    "dest": "cli/azure"
  }
}
