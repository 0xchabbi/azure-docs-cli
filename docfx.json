{
  "build": {
    "content": [
      {
        "src": "docs-ref-autogen",
        "files": [
          "**/*.pycliyml",
          "**/*.md"
        ],
        "exclude": [
          "refTOC.md",
          "**/obj/**",
          "cli/**",
          "**/includes/**"
        ]
      },
      {
        "src": "docs-ref-conceptual",
        "files": [
          "**.md"
        ],
        "exclude":[
          "toc.md",
          "**/toc.md"
        ]
      },
      {
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "files": [
          "**/*.yml"
        ]
      }

    ],
    "resource": [
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/cli/azure/breadcrumb/toc.json",
      "keywords": "azure, cli, command line",
      "brand": "azure",
      "author": "rloutlaw",
      "ms.author": "routlaw",
      "manager": "douge",
      "ms.topic": "reference",
      "ms.devlang": "azurecli",
      "ms.date": "12/06/2016"
    },
    "fileMetadata": {
       "ms.service":
        {
          "docs-ref-autogen/acr/**": "container-registry",
          "docs-ref-autogen/acr.pycliyml": "container-registry",
          "docs-ref-autogen/acs/**": "container-service",
          "docs-ref-autogen/acs.pycliyml": "container-service",
          "docs-ref-autogen/ad/**": "active-directory",
          "docs-ref-autogen/ad.pycliyml": "active-directory",
          "docs-ref-autogen/appservice/**": "app-service",
          "docs-ref-autogen/appservice.pycliyml": "app-service",
          "docs-ref-autogen/container/**": "container-service",
          "docs-ref-autogen/container.pycliyml": "container-service",
          "docs-ref-autogen/group/**": "azure-resource-manager",
          "docs-ref-autogen/group.pycliyml": "azure-resource-manager",
          "docs-ref-autogen/iot/**": "iot-hub",
          "docs-ref-autogen/iot.pycliyml": "iot-hub",
          "docs-ref-autogen/keyvault/**":"key-vault",
          "docs-ref-autogen/keyvault.pycliyml":"key-vault",
          "docs-ref-autogen/network/**":"virtual-network",
          "docs-ref-autogen/network.pycliyml":"virtual-network",
          "docs-ref-autogen/policy/**": "",
          "docs-ref-autogen/policy.pycliyml": "",
          "docs-ref-autogen/redis/**":"redis-cache",
          "docs-ref-autogen/redis.pycliyml":"redis-cache",
          "docs-ref-autogen/resource/**":"azure-resource-manager",
          "docs-ref-autogen/resource.pycliyml":"azure-resource-manager",
          "docs-ref-autogen/role/**": "",
          "docs-ref-autogen/role.pycliyml": "",
          "docs-ref-autogen/sql/**": "sql-server",
          "docs-ref-autogen/sql.pycliyml": "sql-server",
          "docs-ref-autogen/storage/**": "storage",
          "docs-ref-autogen/storage.pycliyml": "storage",
          "docs-ref-autogen/vm/**": "virtual-machines",
          "docs-ref-autogen/vm.pycliyml": "virtual-machines",
          "docs-ref-autogen/vmss/**": "virtual-machine-scale-sets",
          "docs-ref-autogen/vmss.pycliyml": "virtual-machine-scale-sets"    
         },
        "hideEdit":
        {
          "docs-ref-autogen/**": "true"
        }
    },
    "template": [],
    "dest": "cli/azure"
  }
}
